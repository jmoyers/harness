{
  "mux": {
    "keybindings": {
      "mux.conversation.new": [
        "ctrl+t"
      ],
      "mux.conversation.next": [
        "ctrl+j"
      ],
      "mux.conversation.previous": [
        "ctrl+k"
      ],
      "mux.app.interrupt-all": [
        "ctrl+c"
      ]
    },
    "ui": {
      "paneWidthPercent": 35.71,
      "repositoriesCollapsed": false,
      "shortcutsCollapsed": true
    },
    "git": {
      "enabled": true,
      "activePollMs": 1000,
      "idlePollMs": 5000,
      "burstPollMs": 400,
      "burstWindowMs": 2500,
      "triggerDebounceMs": 180,
      "maxConcurrency": 1
    }
  },
  "debug": {
    "enabled": true,
    "overwriteArtifactsOnStart": true,
    "perf": {
      "enabled": true,
      "filePath": ".harness/perf-startup.jsonl"
    },
    "mux": {
      "debugPath": ".harness/mux-debug.jsonl",
      "validateAnsi": false,
      "resizeMinIntervalMs": 33,
      "ptyResizeSettleMs": 75,
      "startupSettleQuietMs": 300,
      "serverSnapshotModelEnabled": true
    }
  },
  "codex": {
    "telemetry": {
      "enabled": true,
      "host": "127.0.0.1",
      "port": 0,
      "logUserPrompt": true,
      "captureLogs": true,
      "captureMetrics": true,
      "captureTraces": true
    },
    "history": {
      "enabled": true,
      "filePath": "~/.codex/history.jsonl",
      "pollMs": 500
    },
    "launch": {
      "defaultMode": "yolo",
      "directoryModes": {}
    }
  },
  "hooks": {
    "lifecycle": {
      "enabled": true,
      "providers": {
        "codex": true,
        "claude": true,
        "controlPlane": true
      },
      "peonPing": {
        "enabled": true,
        "baseUrl": "http://127.0.0.1:19998",
        "timeoutMs": 1200,
        "eventCategoryMap": {
          "turn.started": "task.acknowledge",
          "turn.completed": "task.complete",
          "turn.failed": "task.error",
          "input.required": "input.required",
          "session.started": "session.start"
        }
      },
      "webhooks": []
    }
  }
}
